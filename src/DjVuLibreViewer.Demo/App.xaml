<Application x:Class="DjVuLibreViewer.Demo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DjVuLibreViewer.Demo"
             xmlns:converters="clr-namespace:DjVuLibreViewer.Demo.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <converters:IconToImageSourceConverter x:Key="IconToImageSourceConverter" />
        <converters:MathConverter x:Key="MathConverter" />
        <converters:StringToIntConverter x:Key="StringToIntConverter" />
        <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    </Application.Resources>
</Application>
